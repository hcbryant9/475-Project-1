<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/grid_background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.GridActivity">

    <ImageView
        android:id="@+id/btn33"
        android:layout_width="333dp"
        android:layout_height="133dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="1dp"
        android:contentDescription="@string/button3x3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btnn33" />

    <ImageView
        android:id="@+id/btn44"
        android:layout_width="333dp"
        android:layout_height="144dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:contentDescription="@string/button4x4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.611"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn33"
        app:srcCompat="@drawable/btn44" />

    <ImageView
        android:id="@+id/btn55"
        android:layout_width="330dp"
        android:layout_height="148dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="104dp"
        android:contentDescription="@string/button5x5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.611"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn44"
        app:srcCompat="@drawable/btn55" />

</androidx.constraintlayout.widget.ConstraintLayout>